
@{
    ViewBag.Title = "Quick Add Entry";
    Layout = null;
}
<style>
    body {
        margin: 0;
    }
</style>

<iframe id="iframe" width="100%" style="border:none;height: -webkit-fill-available;"></iframe>
@using System.Web.Optimization
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/jqueryval")

@if (Request.QueryString["Addedfrom"] == "QuickAddSupplier")
{
    <script>
        $("#iframe").attr('src', "../Master/AddSupplier?Addedfrom=@Request.QueryString["Addedfrom"]");
    </script>
}

@if (Request.QueryString["Addedfrom"] == "QuickAddClient")
{
    <script>
        $("#iframe").attr('src', "../Master/AddClient?Addedfrom=@Request.QueryString["Addedfrom"]");
    </script>
}

@if (Request.QueryString["Addedfrom"] == "QuickAddItem")
{
    <script>
        $("#iframe").attr('src', "../Master/AddItem?Addedfrom=@Request.QueryString["Addedfrom"]");
    </script>
}


<script>
    function closew() {
        window.close();
    }
</script>

